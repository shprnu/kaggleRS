     "duration": 0.027937,
     "end_time": "2024-07-28T14:18:37.958483",
     "exception": false,
     "start_time": "2024-07-28T14:18:37.930546",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def weighted_rating(x, m=m, C=C):\n",
    "    v = x['vote_count']\n",
    "    R = x['vote_average']\n",
    "    # Calculation based on the IMDB formula\n",
    "    return (v/(v+m) * R) + (m/(m+v) * C)"
   ]
  },
  {
